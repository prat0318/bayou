JOIN 2
SHOW_DB
SHOW_CLIENTS
START_CLIENT 1
START_CLIENT 2
SHOW_CLIENTS
SHOW_DB
OP client_0 SHOW$
OP client_1 ADD$song1$url1
OP client_1 SHOW$
STOP_CLIENT client_1
SHOW_CLIENTS
SHOW_DB
RESTART_CLIENT client_1
SHOW_CLIENTS
OP client_0 ADD$song2$url1
OP client_0 SHOW$
OP client_1 SHOW$
SHOW_CLIENTS
DISCONNECT_FROM db_0 client_0
OP client_0 ADD$song3$url1
OP client_0 SHOW$
OP client_1 SHOW$
SHOW_CLIENTS
SHOW_DB
RETIRE db_0#1
SHOW_DB
OP client_0 SHOW$
OP client_1 SHOW$
SHOW_CLIENTS
SHOW_DB
#QUIT