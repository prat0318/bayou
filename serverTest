JOIN 2
JOIN 3
SHOW_DB
START_CLIENT db_0
START_CLIENT db_0#1
START_CLIENT db_0#2
START_CLIENT db_0#3
OP client_0 ADD$song1$url1
DISCONNECT_FROM db_0 db_0#1
DISCONNECT_FROM db_0 db_0#2
DISCONNECT_FROM db_0#1 db_0#3
DISCONNECT_FROM db_0#2 db_0#3
SHOW_CLIENTS
OP client_0 ADD$song2$url1
OP client_1 ADD$song3$url1
OP client_2 ADD$song4$url1
OP client_3 ADD$song5$url1
CURR_STATE
DISCONNECT_FROM db_0#1 db_0
#Disconnect between the db's so that the changes
OP client_1 ADD$song1$url1
OP client_1 SHOW$
CURR_STATE
DISCONNECT_FROM db_0 client_0
OP client_0 SHOW$
OP client_0 ADD$song3$url1
CONNECT_THEM db_0 db_0#1
CONNECT_THEM db_0 db_0#2
CONNECT_THEM db_0#1 db_0#3
CONNECT_THEM db_0#2 db_0#3
OP client_0 SHOW$
PAUSE
OP client_0 SHOW$
OP client_0 SHOW$
CONTINUE
OP client_0 SHOW$
QUIT