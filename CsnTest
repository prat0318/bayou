#Testing Retirement of R with other nodes, A and B in picture
JOIN 1
SHOW_DB

START_CLIENT db_0
START_CLIENT db_0#1
SHOW_CLIENTS
DISCONNECT_FROM db_0 db_0#1

OP client_0 ADD$song1$url1
OP client_1 ADD$song2$url2
OP client_0 ADD$song3$url3
OP client_1 ADD$song4$url4
CURR_STATE

#connect db_0#2 and let it gossip to db_0#1
CONNECT_THEM db_0 db_0#1
OP client_0 ADD$song5$url5
CURR_STATE
QUIT
